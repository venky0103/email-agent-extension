{
  "manifest_version": 3,
  "name": "Email Agent",
  "version": "1.0",
  "description": "A Chrome extension for managing Gmail emails.",
  "permissions": ["storage", "identity", "activeTab", "scripting"],
  "host_permissions": ["https://mail.google.com/", "https://www.googleapis.com/"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
